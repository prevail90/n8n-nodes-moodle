{
  "name": "n8n-nodes-moodle",
  "version": "0.1.0",
  "description": "n8n node for Moodle integration",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "murat cakir",
    "email": "muratc@tr66.net"
  },
  "contributors": {
    "name": "Collaborative development with Claude (Anthropic)",
    "url": "https://claude.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/<...>/n8n-nodes-moodle.git"
  },
  "main": "index.js",
  "scripts": {
    "prebuild": "node scripts/generate-version.js",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "version:patch": "npm version patch && npm run build",
    "version:minor": "npm version minor && npm run build",
    "version:major": "npm version major && npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MoodleApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Moodle/Moodle.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.16",
    "@typescript-eslint/parser": "^5.59.7",
    "eslint-plugin-n8n-nodes-base": "^1.12.1",
    "gulp": "^5.0.1",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
